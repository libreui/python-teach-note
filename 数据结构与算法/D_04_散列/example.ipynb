   "source": [
    "# 异序词散列函数, 用字符位置作为权重，避免相同字符的字符串散列值相同\n",
    "def hash(s, table_size):\n",
    "    sum = 0\n",
    "    for pos in range(len(s)):\n",
    "        sum += ord(s[pos]) * pos\n",
    "    return sum % table_size\n",
    "\n",
    "print(hash('cat', 11))\n",
    "print(hash('act', 11))\n",
    "print(hash('tac', 11))\n"
   ],
