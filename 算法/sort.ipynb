    "# 冒泡排序\n",
    "import numpy as np\n",
    "list = np.random.randint(100, size=10)\n",
    "for i in range(0, len(list)):\n",
    "    for j in range(0, len(list)-i-1):\n",
    "        if (list[j] > list[j+1]):\n",
    "            list[j], list[j+1] = list[j+1], list[j]\n",
    "print(list)"
   ]
  }
